<Window x:Class="Kamalov_AutoService.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Kamalov_AutoService"
        mc:Ignorable="d"
        Title="Kamalov.AutoService" Height="450" Width="800" FontFamily="Tw Cen MT" Icon="Resources/service_logo.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>

        <Image Source="Resources/service_logo.png" HorizontalAlignment="Left"></Image>
        <TextBlock Text="Услуги автосервиса" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
        <Frame Grid.Row="1" NavigationUIVisibility="Hidden" Name="MainFrame" ContentRendered="MainFrame_ContentRendered"></Frame>
        <Button Content="Назад" Name="BtnBack"  HorizontalAlignment="Right" Click="Button_Click"/>
    
    </Grid>
</Window>
